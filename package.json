{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@mdx-js/mdx": "^1.5.3",
    "@mdx-js/react": "^1.5.3",
    "@mdx-js/tag": "^0.20.3",
    "gatsby": "^2.18.12",
    "gatsby-image": "^2.2.34",
    "gatsby-link": "^2.2.27",
    "gatsby-paginate": "^1.1.1",
    "gatsby-plugin-feed": "^2.3.25",
    "gatsby-plugin-google-analytics": "^2.1.31",
    "gatsby-plugin-manifest": "^2.2.31",
    "gatsby-plugin-mdx": "^1.0.64",
    "gatsby-plugin-offline": "^3.0.27",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-sharp": "^2.3.10",
    "gatsby-plugin-sitemap": "^2.2.24",
    "gatsby-plugin-styled-components": "^3.1.16",
    "gatsby-plugin-typescript": "^2.1.23",
    "gatsby-remark-copy-linked-files": "^2.1.33",
    "gatsby-remark-images": "^3.1.39",
    "gatsby-source-filesystem": "^2.1.43",
    "gatsby-transformer-react-docgen": "^5.0.25",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^2.3.9",
    "grommet": "^2.9.0",
    "grommet-icons": "^4.4.0",
    "prism-react-renderer": "^1.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-live": "^2.2.2",
    "slugify": "^1.3.6",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "typescript": "^3.7.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf .cache && npm run develop",
    "develop": "nodemon --exec \"gatsby develop\"",
    "debug": "nodemon --exec \"node --inspect-brk --no-lazy node_modules/.bin/gatsby develop\"",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "eslint . --ext ts --ext tsx",
    "storybook": "start-storybook",
    "storybook:build": "build-storybook -c .storybook -o .out",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "nodemonConfig": {
    "ignore": [
      ".cache/*",
      "public/*",
      "schema.json",
      "src/*",
      "content/*",
      "static/*"
    ],
    "delay": "1500"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
